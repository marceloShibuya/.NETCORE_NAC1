@model Cliente

<div class="form-group">
    <label asp-for="Nome"></label>
    <input asp-for="Nome" class="form-control" />
</div>
<div class="form-group">
    <label asp-for="DtNascimento"></label>
    <input asp-for="DtNascimento" class="form-control" />
</div>

<div class="form-group">
    <label asp-for="Sexo"></label>
    <select asp-for="Sexo" asp-items="@Html.GetEnumSelectList<Sexo>()" class="form-control">
        <option>Selecione</option>
    </select>
</div>

<div class="form-group">
    <label asp-for="TipoConta"></label>
    <select asp-for="TipoConta" class="form-control" asp-items="@ViewBag.conta">
        <option>Selecione</option>
    </select>
</div>

<div class="form-group">
    <label asp-for="VlDeposito"></label>
    <input asp-for="VlDeposito" class="form-check" />
</div>

<div>
    <input asp-for="NewsLetter" />
    <label asp-for="NewsLetter"></label>
</div>